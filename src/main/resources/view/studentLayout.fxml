<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="tk.aizydorczyk.gradebook.controller.StudentController">
    <center>
        <TableView id="studentTableView" fx:id="studentTableView" prefHeight="200.0" prefWidth="200.0"
                   BorderPane.alignment="CENTER"/>
    </center>
    <left>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button id="studentSubjectsButton" fx:id="studentSubjectsButton" mnemonicParsing="false"
                        onAction="#studentSubjectsButtonClick" prefHeight="25.0" prefWidth="109.0"
                        text="%studentSubjects" GridPane.rowIndex="1"/>
                <Button id="studentGradesButton" fx:id="studentGradesButton" mnemonicParsing="false"
                        onAction="#studentGradesButtonClick" prefHeight="25.0" prefWidth="106.0" text="%studentGrades"
                        GridPane.rowIndex="2"/>
                <Button id="studentGroupsButton" fx:id="studentGroupsButton" mnemonicParsing="false"
                        onAction="#studentGroupsButtonClick" prefHeight="25.0" prefWidth="114.0" text="%studentGroups"
                        GridPane.rowIndex="3"/>
                <Button id="studentMembersOfGroup" fx:id="studentMembersOfGroup" mnemonicParsing="false"
                        onAction="#studentMembersOfGroupClick" prefHeight="25.0" prefWidth="118.0"
                        text="%studentMembers" GridPane.rowIndex="4"/>
                <Button id="studentLecturersButton" fx:id="studentLecturersButton" mnemonicParsing="false"
                        onAction="#studentLecturersButtonClick" prefHeight="25.0" prefWidth="127.0"
                        text="%studentLecturers" GridPane.rowIndex="5"/>
                <Button id="studentSemestersButton" fx:id="studentSemestersButton" mnemonicParsing="false"
                        onAction="#studentSemestersButtonClick" prefHeight="25.0" prefWidth="108.0"
                        text="%studentSemesters"/>
            </children>
        </GridPane>
    </left>
    <top>
        <Pane prefHeight="36.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button id="logoutButton" fx:id="logoutButton" layoutX="510.0" layoutY="6.0" mnemonicParsing="false"
                        onAction="#logoutButtonClick" prefHeight="25.0" prefWidth="76.0" text="%logout"/>
            </children>
        </Pane>
    </top>
</BorderPane>
